<Window x:Class="Restaurant.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Restaurant"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <ComboBox x:Name="dateBox" HorizontalAlignment="Left" Margin="61,30,0,0" VerticalAlignment="Top" Width="120">

        </ComboBox>
        <ComboBox x:Name="personsBox" HorizontalAlignment="Left" Height="25" Margin="361,200,0,0" VerticalAlignment="Top" Width="37">

        </ComboBox>
        <ComboBox x:Name="timeBox" HorizontalAlignment="Left" Height="26" Margin="61,58,0,0" VerticalAlignment="Top" Width="120">
        </ComboBox>
        <TextBox x:Name="nameBox" HorizontalAlignment="Left" Height="25" Margin="65,149,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="phoneBox" HorizontalAlignment="Left" Height="25" Margin="65,213,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="nameLabel" Content="Имя" HorizontalAlignment="Left" Height="30" Margin="61,129,0,0" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="phoneLabel" Content="Телефон" HorizontalAlignment="Left" Height="36" Margin="61,189,0,0" VerticalAlignment="Top" Width="92"/>
        <Button x:Name="adminButton" Content="admin" HorizontalAlignment="Left" Height="76" Margin="361,30,0,0" VerticalAlignment="Top" Width="106" Click="adminButton_Click"/>
        <Button x:Name="completeButton" Content="complete" HorizontalAlignment="Left" Height="51" Margin="361,249,0,0" VerticalAlignment="Top" Width="106" Click="completeButton_Click"/>
        <Label x:Name="label" Content="Количество людей" HorizontalAlignment="Left" Height="36" Margin="281,171,0,0" VerticalAlignment="Top" Width="135"/>

    </Grid>
</Window>
